node{
    stage('SCM'){
        git branch:'${branch}',url:'https://github.com/rahulbhadani457/testJenkins.git'
        println("hello SCM")
    }
    stage('build'){
        steps{
            sh python abc.py
        }
        println("hello")
    }
    stage('Test'){
        println("test2")
    }
    stage('deploy'){
        println("hello deploy")
    }

}